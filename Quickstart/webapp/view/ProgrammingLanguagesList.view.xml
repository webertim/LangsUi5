<mvc:View
   controllerName="sap.ui.demo.walkthrough.controller.ProgrammingLanguagesList"
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc">
   <Table id="ProgrammingLanguagesTable" inset="true" itemPress="onLanguageClicked">
	   	<headerToolbar>
	         <Toolbar>
	            <Title text="{i18n>programmingLanguagesListTitle}"/>
	            <ToolbarSpacer/>
	            <SearchField width="50%" search=".onFilterProgrammingLanguages"/>
	         </Toolbar>
	      </headerToolbar>
   		<columns>
   			<Column minScreenWidth="Medium" demandPopin="true" hAlign="Left" >
   				<Text text="Name"></Text>
   			</Column>
   			<Column minScreenWidth="Medium" demandPopin="true" hAlign="Left">
   				<Text text="OperationArea"></Text>
   			</Column>
   			<Column minScreenWidth="Medium" demandPopin="true" hAlign="Left">
   				<Text text="Release"></Text>
   			</Column>
   			<Column minScreenWidth="Medium" demandPopin="true" hAlign="Left">
   				<Text text="Popularity"></Text>
   			</Column>
   			<Column minScreenWidth="Medium" demandPopin="true" hAlign="Left">
   				<Text text="GettingStarted"></Text>
   			</Column>
   			<Column minScreenWidth="Medium" demandPopin="true" hAlign="Left">
   				<Text text="Documentation"></Text>
   			</Column>
   			<Column minScreenWidth="Medium" demandPopin="true" hAlign="Left">
   				<Text text="UserRating"></Text>
   			</Column>
   		</columns>
   </Table>
</mvc:View>